{
  "name": "@nuxtjs/color-mode",
  "version": "3.5.2",
  "description": "Dark and light mode for Nuxt with auto detection",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/color-mode.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "contributors": [
    {
      "name": "Nuxt Team"
    }
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build && esbuild --minify src/script.js --outfile=dist/script.min.js",
    "build": "pnpm run prepack",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt generate docs",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run prepack",
    "release": "pnpm lint && pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/test-utils": "^3.14.4",
    "@types/lodash.template": "^4.5.3",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitest/coverage-v8": "^4.0.8",
    "changelogen": "^0.5.7",
    "eslint": "^9.13.0",
    "husky": "9.1.6",
    "nuxt": "^4.2.1",
    "typescript": "^5.6.3",
    "vitest": "^4.0.8",
    "vue-tsc": "^2.1.6"
  }
}
